{
    "name": "dv5150/shop-filament",
    "description": "Filament support for Laravel shop package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "DV5150\\Shop\\Filament\\": "src/",
            "DV5150\\Shop\\Filament\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "David Szabados",
            "email": "dv5150dev@gmail.com"
        }
    ],
    "repositories": {
        "shop": {
            "type": "path",
            "url": "../shop",
            "options": {
                "symlink": false
            }
        }
    },
    "require": {
        "php": "^8.0",
        "dv5150/shop": "@dev",
        "filament/filament": "^2.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "extra": {
        "laravel": {
            "providers": [
                "DV5150\\Shop\\Filament\\ShopFilamentServiceProvider"
            ]
        }
    }
}
